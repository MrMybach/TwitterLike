<!DOCTYPE html>
<html>
<head>
  <title>TwitterLike</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
	<header>
		<%= render 'layouts/navigation' %>
	</header>
	<main role="main">
		<div class="col-md-10 col-md-offset-1">

			<div class="row">
				<div class="col-md-6 col-md-offset-3">
					<%= flash_messages %>
				</div>
			</div>

			<%= yield %>
		</div>
	</main>
</body>
</html>
