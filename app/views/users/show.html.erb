<h1>You are waching <span><%= @user.email %></span> Tweets</h1>
<div class="row">
	<div class="col-md-6 col-md-offset-2">
		<% @user.posts.each do |post| %>
			<div class="post ">
				<h2><%= post.title %></h2>
				<small> <%= post.created_at.strftime("%d %b %Y") %></small>
				<p><%= post.text %></p>
			</div>
			<hr>
		<% end %>
	</div>
</div>