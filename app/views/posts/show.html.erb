<div class="row">
	<h1><%= @post.title %></h1>
	<p><%= @post.text %></p>
</div>
<div class="row">
	<div class="btn-group btn-group-xs" role="group">
		<%= link_to "Edit", edit_post_path, class: "btn btn-default" %>
		<%= link_to "Delete", post_path(@post), method: :delete, class: "btn btn-default" %>
	</div>
</div>