<div class="row">
	<h1>Posts Index</h1>
</div>

<div class="row">
	<% @posts.each do |post| %>
		<div class="post col-md-4">
			<h2><%= link_to "#{post.title}", post_path(post) %></h2>
			<small> <%= post.created_at.strftime("%d %b %Y") %></small>
			<p><%= post.text %></p>
		</div>
	<% end %>
</div>